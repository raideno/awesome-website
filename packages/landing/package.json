{
  "name": "landing",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env -- vite --port 3000",
    "start": "dotenv -e .env -- vite --port 3000",
    "build": "dotenv -e .env -- vite build && tsc",
    "serve": "dotenv -e .env -- vite preview",
    "lint": "eslint",
    "test": "dotenv -e .env -- vitest run",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dotenv-cli": "^9.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.0.5"
  }
}