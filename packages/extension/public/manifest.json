{
    "manifest_version": 3,
    "name": "My Side Panel Extension",
    "version": "1.0.0",
    "permissions": [
        "sidePanel",
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "Open side panel"
    },
    "background": {
        "service_worker": "background.js"
    },
    "side_panel": {
        "default_path": "index.html"
    }
}